{
  "manifest_version": 3,
  "name": "BeeYond",
  "version": "1.0.0",
  "description": "Helpfull tools for Logicommerce",
  "icons": {
    "16": "/dist/beeyond.png",
    "48": "/dist/beeyond.png",
    "128": "/dist/beeyond.png"
  },
  "action": {
    "default_title": "BeeYond",
    "default_popup": "/dist/popup.html"
  },
  "background": {
    "service_worker": "/dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.google.com/*"
      ],
      "js": [
        "/dist/contentScript.js"
      ]
    }
  ],
  "options_page": "/dist/options.html"
}